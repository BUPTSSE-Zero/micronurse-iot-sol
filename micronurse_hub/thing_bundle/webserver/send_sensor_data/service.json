{
  "description":"Send the sensor data to web server",
  "spec": {
    "id":"webserver_send_sensor_data",
    "name": "send sensor data",
    "data_cache_size": 5,

    "config": [{
      "name": "timeout",
      "type": "int",
      "default": 5000
    },{
      "name": "user_phone_num",
      "type": "string",
      "default": "123456"
    },{
      "name": "password",
      "type": "string",
      "default": "123456"
    }],

    "in": {
      "ports": [{
        "name": "json_data",
        "type": "string"
      }]
    }
  }
}
