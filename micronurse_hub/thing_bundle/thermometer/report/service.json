{
  "description":"report the temprature at intervals",
  "spec": {
    "id":"thermometer_report",
    "name": "thermometer report",

    "config": [{
      "name": "interval",
      "type": "int",
      "default": 2000
    },{
      "name": "name",
      "type": "string",
      "default": "thermometer"
    }],

    "out": {
      "ports": [{
        "name": "value",
        "type": "int"
      },{
        "name": "name",
        "type": "string"
      },{
        "name": "timestamp",
        "type": "long"
      },{
        "name": "json_data",
        "type": "string"
      }]
    }
  }
}
